    "@types/react-dom": "^18.3.0",
    "@uidotdev/usehooks": "^2.4.1",
    "antd": "^5.19.1",
    "axios": "^1.7.2",
    "cypress": "^13.13.1",
    "ehsaantech-react-image-annotation": "^0.9.12",
    "eslint": "8.57",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-drag-drop-files": "^2.3.10",
